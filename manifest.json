{
  "description": "Counts the amount of cybers in a website",
  "manifest_version": 2,
  "name": "CYBER-COUNTER",
  "version": "1.0",
  "homepage_url": "https://github.com/heddha/cyber-counter",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["functions.js"]
    }
  ]

}
